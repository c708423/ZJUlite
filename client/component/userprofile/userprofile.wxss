/* component/userprofile/userprofile.wxss */
.userprofile-class {
  width:100%;
  height:182rpx;
  display:flex;
  margin-bottom: 9rpx;
  background-color:white;
  position:relative;
  align-items: center;
}
.userprofile-class button {
position:absolute;
width:750rpx;
height:100%;
opacity:0;

}
.user-head {
  height:182rpx;
  width:182rpx;
  margin-right:10rpx;
  display:flex;
  align-items: center;
}
.headimg-wrap{
  height:120rpx;
  width:120rpx;
  border-radius: 50%;
  background-color:#DFDFDF;
  margin:0 auto;
  overflow:hidden;
  display:flex;
  align-items:center;
  justify-content:center;
}
.user-name {
  font-size:34rpx;
  color:#000000;
  padding-top:38rpx;
}
.is-register{
  display: flex;
  direction: row;
  font-size: 26rpx;
}
.is-register .text{
  color: #353535;
}
.is-register .link{
  color: #0eb9e7;
}
.rightbox{
  display: block;
}
.unlogginimg{
  width:70%;
  height:70%;
}